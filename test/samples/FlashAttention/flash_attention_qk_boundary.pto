module {
  func.func @flash_attention_qk_boundary_block(%q: !pto.ptr<f32>, %kt: !pto.ptr<f32>, %scores: !pto.ptr<f32>, %valid_m: i32, %valid_k: i32, %valid_n: i32) {
    %c0 = arith.constant 0 : index
    %c1 = arith.constant 1 : index
    %c32 = arith.constant 32 : index

    %vm = arith.index_cast %valid_m : i32 to index
    %vk = arith.index_cast %valid_k : i32 to index
    %vn = arith.index_cast %valid_n : i32 to index

    %q_tv = pto.make_tensor_view %q, shape = [%c32, %c32] strides = [%c32, %c1] : !pto.tensor_view<?x?xf32>
    %kt_tv = pto.make_tensor_view %kt, shape = [%c32, %c32] strides = [%c32, %c1] : !pto.tensor_view<?x?xf32>
    %scores_tv = pto.make_tensor_view %scores, shape = [%c32, %c32] strides = [%c32, %c1] : !pto.tensor_view<?x?xf32>

    %q_pt = pto.partition_view %q_tv, offsets = [%c0, %c0], sizes = [%c32, %c32] : !pto.tensor_view<?x?xf32> -> !pto.partition_tensor_view<32x32xf32>
    %kt_pt = pto.partition_view %kt_tv, offsets = [%c0, %c0], sizes = [%c32, %c32] : !pto.tensor_view<?x?xf32> -> !pto.partition_tensor_view<32x32xf32>
    %scores_pt = pto.partition_view %scores_tv, offsets = [%c0, %c0], sizes = [%c32, %c32] : !pto.tensor_view<?x?xf32> -> !pto.partition_tensor_view<32x32xf32>

    %q_mat = pto.alloc_tile valid_row = %vm valid_col = %vk : !pto.tile_buf<loc=mat, dtype=f32, rows=32, cols=32, v_row=?, v_col=?, blayout=col_major, slayout=row_major, fractal=512, pad=0>
    %kt_mat = pto.alloc_tile valid_row = %vk valid_col = %vn : !pto.tile_buf<loc=mat, dtype=f32, rows=32, cols=32, v_row=?, v_col=?, blayout=col_major, slayout=row_major, fractal=512, pad=0>
    %q_left = pto.alloc_tile valid_row = %vm valid_col = %vk : !pto.tile_buf<loc=left, dtype=f32, rows=32, cols=32, v_row=?, v_col=?, blayout=row_major, slayout=row_major, fractal=512, pad=0>
    %k_right = pto.alloc_tile valid_row = %vk valid_col = %vn : !pto.tile_buf<loc=right, dtype=f32, rows=32, cols=32, v_row=?, v_col=?, blayout=row_major, slayout=col_major, fractal=512, pad=0>
    %scores_acc = pto.alloc_tile valid_row = %vm valid_col = %vn : !pto.tile_buf<loc=acc, dtype=f32, rows=32, cols=32, v_row=?, v_col=?, blayout=col_major, slayout=row_major, fractal=1024, pad=0>

    pto.tload ins(%q_pt : !pto.partition_tensor_view<32x32xf32>) outs(%q_mat : !pto.tile_buf<loc=mat, dtype=f32, rows=32, cols=32, v_row=?, v_col=?, blayout=col_major, slayout=row_major, fractal=512, pad=0>)
    pto.tload ins(%kt_pt : !pto.partition_tensor_view<32x32xf32>) outs(%kt_mat : !pto.tile_buf<loc=mat, dtype=f32, rows=32, cols=32, v_row=?, v_col=?, blayout=col_major, slayout=row_major, fractal=512, pad=0>)

    pto.tmov ins(%q_mat : !pto.tile_buf<loc=mat, dtype=f32, rows=32, cols=32, v_row=?, v_col=?, blayout=col_major, slayout=row_major, fractal=512, pad=0>) outs(%q_left : !pto.tile_buf<loc=left, dtype=f32, rows=32, cols=32, v_row=?, v_col=?, blayout=row_major, slayout=row_major, fractal=512, pad=0>)
    pto.tmov ins(%kt_mat : !pto.tile_buf<loc=mat, dtype=f32, rows=32, cols=32, v_row=?, v_col=?, blayout=col_major, slayout=row_major, fractal=512, pad=0>) outs(%k_right : !pto.tile_buf<loc=right, dtype=f32, rows=32, cols=32, v_row=?, v_col=?, blayout=row_major, slayout=col_major, fractal=512, pad=0>)

    pto.tmatmul ins(%q_left, %k_right : !pto.tile_buf<loc=left, dtype=f32, rows=32, cols=32, v_row=?, v_col=?, blayout=row_major, slayout=row_major, fractal=512, pad=0>, !pto.tile_buf<loc=right, dtype=f32, rows=32, cols=32, v_row=?, v_col=?, blayout=row_major, slayout=col_major, fractal=512, pad=0>) outs(%scores_acc : !pto.tile_buf<loc=acc, dtype=f32, rows=32, cols=32, v_row=?, v_col=?, blayout=col_major, slayout=row_major, fractal=1024, pad=0>)
    pto.tstore ins(%scores_acc : !pto.tile_buf<loc=acc, dtype=f32, rows=32, cols=32, v_row=?, v_col=?, blayout=col_major, slayout=row_major, fractal=1024, pad=0>) outs(%scores_pt : !pto.partition_tensor_view<32x32xf32>)
    return
  }
}
